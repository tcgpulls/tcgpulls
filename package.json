    "@aws-sdk/lib-storage": "^3.714.0",
    "axios": "^1.7.9",
    "dotenv-cli": "^7.4.4",
    "p-limit": "^6.1.0",
    "sharp": "^0.33.5",
    "ts-node": "^10.9.2",
    "tsx": "^4.19.2"
  },
