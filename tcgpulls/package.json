{
  "name": "tcgpulls",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "prisma generate && next build",
    "start": "next start",
    "lint": "next lint",
    "test": "jest",
    "test:watch": "jest --watch",
    "next:clear": "rm -rf .next",
    "prisma:generate": "dotenv -e .env.local -- npx prisma generate",
    "prisma:studio": "dotenv -e .env.local -- npx prisma studio",
    "prisma:db-migrate-dev": "dotenv -e .env.local -- npx prisma migrate dev",
    "prisma:db-pull": "dotenv -e .env.local -- npx prisma db pull",
    "prisma:db-reset": "dotenv -e .env.local -- npx prisma migrate reset --force",
    "pokemon:fetch-store-sets": "dotenv -e .env.local -- npx tsx scripts/fetchAndStorePokemonSets.ts",
    "pokemon:fetch-store-sets-with-image": "dotenv -e .env.local -- npx tsx scripts/fetchAndStorePokemonSets.ts -- --forceImageDownload",
    "pokemon:clear-sets": "dotenv -e .env.local -- tsx scripts/clearPokemonSets.ts"
  },
  "dependencies": {
    "@formatjs/intl-localematcher": "^0.5.9",
    "@headlessui/react": "^2.2.0",
    "@heroicons/react": "^2.2.0",
    "@playwright/test": "^1.49.0",
    "@prisma/client": "^6.0.1",
    "axios": "^1.7.9",
    "clsx": "^2.1.1",
    "framer-motion": "^11.13.1",
    "js-cookie": "^3.0.5",
    "next": "15.0.3",
    "next-intl": "^3.26.1",
    "playwright": "^1.49.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.1.0",
    "@types/js-cookie": "^3.0.6",
    "@types/negotiator": "^0.6.3",
    "@types/node": "^20.17.9",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "dotenv-cli": "^7.4.4",
    "eslint": "^8",
    "eslint-config-next": "15.0.3",
    "eslint-config-prettier": "^9.1.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "postcss": "^8",
    "prettier": "^3.4.2",
    "prisma": "^6.0.1",
    "tailwindcss": "^3.4.1",
    "ts-node": "^10.9.2",
    "tsx": "^4.19.2",
    "typescript": "^5.7.2"
  }
}
